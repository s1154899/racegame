<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:raceWPF"
        xmlns:Controller="clr-namespace:Controller;assembly=Controller" x:Class="raceWPF.StatisticsPanel"
        mc:Ignorable="d"
        Title="StatisticsPanel" Height="450" Width="800">
    <Window.DataContext>
        <Controller:AboutCompetition/>
    </Window.DataContext>
    <Grid>
        <TextBox Text="{Binding drivers}" HorizontalAlignment="Left" Margin="0,0,0,10" Width="357" ScrollViewer.HorizontalScrollBarVisibility="Visible" ScrollViewer.CanContentScroll="True" IsReadOnly="True" VerticalScrollBarVisibility="Visible" />
        </Grid>
</Window>
